{
  "name": "@spydersoft/react-runtime-config",
  "version": "3.0.2",
  "description": "Provide a typesafe runtime configuration inside a react app",
  "repository": {
    "type": "git",
    "url": "git@github.com:spyder007/react-runtime-config.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "jest --watch",
    "test": "jest",
    "build": "tsc -p tsconfig.package.json",
    "ci": "yarn test --ci && yarn build",
    "format": "eslint src/*.{ts,tsx} --fix && prettier src/*.{ts,tsx,json} --write"
  },
  "keywords": [
    "typescript",
    "react",
    "config",
    "configuration",
    "runtime"
  ],
  "author": "Fabien Bernard <fabien@contiamo.com>",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.197",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-extended": "^4.0.1",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.1",
    "ts-mockery": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "packageManager": "yarn@3.6.0"
}
